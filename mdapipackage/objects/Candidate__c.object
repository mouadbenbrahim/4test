    <validationRules>
        <fullName>Zip_Code_match_the_state</fullName>
        <active>true</active>
        <errorConditionFormula>AND (VLOOKUP( $ObjectType.Zip_Code__c.Fields.State__c ,  $ObjectType.Zip_Code__c.Fields.Name ,  Zip_Postal_Code__c ) &lt;&gt;  State_Province__c, NOT(Batch_Load_Item__c))</errorConditionFormula>
        <errorDisplayField>Zip_Postal_Code__c</errorDisplayField>
        <errorMessage>Candidate Zip Code does not exist in specified State.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Zip_code_must_be_Valid_US_Postal_Code</fullName>
        <active>true</active>
        <errorConditionFormula>(Country__c == &quot;USA&quot; || Country__c == &quot;US&quot;) &amp;&amp; NOT(REGEX(Zip_Postal_Code__c,&quot;\\d{5}(-\\d{4})?&quot;))</errorConditionFormula>
        <errorDisplayField>Zip_Postal_Code__c</errorDisplayField>
        <errorMessage>Zip code must be in 99999 or 99999-9999 format</errorMessage>
    </validationRules>
